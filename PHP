    <input type="text" name="name" id="name">
    <button type="submit">Submit</button>
</form>

<?php if ($error): ?>
    <p style="color: red;"><?php echo $error; ?></p>
<?php elseif ($greeting): ?>
    <p><?php echo $greeting; ?></p>
<?php endif; ?>

</body>
</html>

