        $qty1 = $_POST['qty1'];
        $price2 = $_POST['price2'];
        $qty2 = $_POST['qty2'];
        $price3 = $_POST['price3'];
        $qty3 = $_POST['qty3'];

       
        $subtotal = ($price1 * $qty1) + ($price2 * $qty2) + ($price3 * $qty3);

        $tax = $subtotal * 0.10;
        $total = $subtotal + $tax;

        echo "<h3>Results:</h3>";
        echo "Subtotal: $" . number_format($subtotal, 2) . "<br>";
        echo "Tax (10%): $" . number_format($tax, 2) . "<br>";
        echo "Total Cost: $" . number_format($total, 2);
    }
    ?>
</body>
</html>

